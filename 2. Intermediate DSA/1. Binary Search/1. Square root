#include <bits/stdc++.h>
int floorSqrt(int n)
{
    // Write your code here.
    long long ans  = 0;
    for (long long i = 1; i<=n; i++) {
        if (i*i <= n){
            ans = max (ans, i);
        }
        else if (i*i> n) break;
    }
    return int (ans);
}

-----------------------2nd code-----------------------

int floorSqrt(int n){
    long long low = 1;
    long long high = n;
    
    while (low<= high) {
        long long mid = (high + low)/2;
        long long val = mid*mid;
        if (val<=n){
            low = mid +1;
        }
        else {
            high = mid - 1;
        }
    }
    return int(high);
}




