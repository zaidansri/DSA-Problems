#include <bits/stdc++.h>

pair<int,int> missingAndRepeating(vector<int> &arr, int n)
{
	// Write your code here
	sort (arr.begin(), arr.end());
	pair<int,int> ans;

	int m = n;
	int r = 0;

	for (int i = 0; i<n-1; i++) {

		if (arr[i+1] - arr[i] == 0) 
			r = arr[i];

		else if (arr[i+1] - arr[i] != 1) 
			m = arr[i]+1;

		else if (arr[0] == 2)
			m = 1;
	}
	ans.first = m;
	ans.second = r;

	return ans;
	
}
